{{require GLib version "2.0"}}
{{require Foundry version "1"}}

{{if node.node_type == Foundry.GirNodeType.REPOSITORY}}
{{repository = node}}{{include "repository.tmpl"}}
{{else if node.node_type == Foundry.GirNodeType.CONSTRUCTOR}}
{{constructor = node}}{{include "constructor.tmpl"}}
{{else if node.node_type == Foundry.GirNodeType.FUNCTION_MACRO}}
{{function_macro = node}}{{include "function-macro.tmpl"}}
{{else if node.node_type == Foundry.GirNodeType.FUNCTION}}
{{function = node}}{{include "function.tmpl"}}
{{end}}
